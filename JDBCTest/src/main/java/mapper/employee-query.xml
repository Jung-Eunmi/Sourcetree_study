<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <entry key="SelectEmployee1">
        SELECT
            EMP_ID,
            EMP_NAME
        FROM
            EMPLOYEE
    </entry>

    <entry key="SelectEmployee2">
        SELECT
            EMP_NAME
        FROM
            EMPLOYEE
        WHERE
            DEPT_CODE = 'D9'
    </entry>
</properties>
